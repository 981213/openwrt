/dts-v1/;

#include "mt7628an.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "hiwifi,hc5661a-nand", "mediatek,mt7628an-soc";
	model = "HiWiFi HC5661A with SPI NAND";

	aliases {
		led-boot = &led_system;
		led-failsafe = &led_system;
		led-running = &led_system;
		led-upgrade = &led_system;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	gpio-leds {
		compatible = "gpio-leds";

		led_system: system {
			label = "hc5661a-nand:blue:system";
			gpios = <&gpio1 5 GPIO_ACTIVE_HIGH>;
		};
		internet {
			label = "hc5661a-nand:blue:internet";
			gpios = <&gpio0 1 GPIO_ACTIVE_HIGH>;
		};
		wlan2g {
			label = "hc5661a-nand:blue:wlan2g";
			gpios = <&gpio1 12 GPIO_ACTIVE_HIGH>;
		};
	};

	gpio-keys {
		compatible = "gpio-keys-polled";
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio1 6 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "i2c", "refclk", "wled_an";
			ralink,function = "gpio";
		};
	};
};
/*
&sdhci {
	status = "okay";
};
*/
&spi0 {
	status = "okay";
	pinctrl-1 = <&spi_cs1_pins>;

	m25p80@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		linux,modalias = "m25p80", "w25q128";
		spi-max-frequency = <45000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "hw_panic";
				reg = <0x30000 0x10000>;
				read-only;
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};

			partition@50000 {
				label = "kernel";
				reg = <0x50000 0xf70000>;
			};

			partition@fc0000 {
				label = "oem";
				reg = <0xfc0000 0x20000>;
				read-only;
			};

			bdinfo: partition@fe0000 {
				label = "bdinfo";
				reg = <0xfe0000 0x10000>;
				read-only;
			};

			partition@ff0000 {
				label = "backup";
				reg = <0xff0000 0x10000>;
				read-only;
			};
		};
	};
	spinand@1 {
		compatible = "spi-nand";
		reg = <1>;
		spi-max-frequency = <45000000>;
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "ubi";
				//exclude the last block which is always read only
				reg = <0x0 0x7fe0000>;
			};
		};
	};
};

&ethernet {
	mtd-mac-address = <&factory 0x4>;
	mediatek,portmap = "wllll";
};

&wmac {
	status = "okay";
};
